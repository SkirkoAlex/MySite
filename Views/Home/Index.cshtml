@model TextField
@{ 
    ViewBag.Title = Model.MetaTitle;
    ViewBag.Description = Model.MetaDescription;
    ViewBag.Keywords = Model.MetaKeywords;
}

<div style="font-family: 'Times New Roman', Times, serif; font-size: 18px; text-shadow: 4px 4px 4px #aaa; font-variant: normal">
@Html.Raw(Model.Text)
</div>